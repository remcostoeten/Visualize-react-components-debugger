{
    "compilerOptions": {
        "jsx": "react-jsx",
        "strict": false,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "target": "ES2020",
        "moduleResolution": "node",
        "module": "commonjs",
        "declaration": true,
        "isolatedModules": false,
        "noEmit": false,

        "outDir": "dist",    "declarationDir": "./dist"

    },
    "include": ["src"],
    "exclude": ["node_modules"]
}